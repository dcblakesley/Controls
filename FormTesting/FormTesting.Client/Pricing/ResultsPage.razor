@page "/pricing/results"
@page "/pricing/results/{CategoryCode}"
<PageTitle>Results Example</PageTitle>
<PricingNavigationControl/>

@if (string.IsNullOrWhiteSpace(CategoryCode))
{
    <h5>Choose a Results group</h5>
    <div class="flex-row">
        <style>
        .card {
            padding: 1rem;
            margin-bottom: 1rem;
            margin-right: 1rem;
        }
    </style>
        <!-- Category -->
        <div class="card">
            <h6 class="text-warning">Just IDS Categories?</h6>

            <!-- Select Category Level -->
            <div class="form-group">
                <label>Category Levels</label>
                <div></div>
                <select>
                    @foreach (var t in _categoryLevels)
                    {
                        <option>@t</option>
                    }
                </select>
            </div>

            <!-- Select Category Name -->
            <div class="form-group">
                <label>Category Names</label>
                <div></div>
                <select>
                    @foreach (var t in _categoryNames)
                    {
                        <option>@t</option>
                    }
                </select>
            </div>

            <a href="/pricing/results/categoryWasSelected" class="btn btn-secondary" @onclick=SelectCategory>Select</a>
        </div>

        <!-- Vendors -->
        <div class="card">
            <div class="form-group">
                <label>Vendors</label>
                <div></div>
                <select>
                    @foreach (var t in _vendors)
                    {
                        <option>@t</option>
                    }
                </select>
            </div>

           <div class="flex-fill"></div>

            <a href="/pricing/results/vendorWasSelected" class="btn btn-secondary" @onclick=SelectCategory>Select</a>
        </div>

        <!-- Role -->
        <div class="card">
            <h6>Roles</h6>
            <p>
                Each would only be visible if the user was in one of the roles
            </p>
            <div class="form-group">
                <label>President</label>
                <div></div>
                <select>
                    @foreach (var t in _categoryNames)
                    {
                        <option>@t</option>
                    }
                </select>
            </div>

            <div class="form-group">
                <label>VP</label>
                <div></div>
                <select>
                    @foreach (var t in _categoryNames)
                    {
                        <option>@t</option>
                    }
                </select>
            </div>

            <div class="form-group">
                <label>Category Manager</label>
                <div></div>
                <select>
                    @foreach (var t in _categoryNames)
                    {
                        <option>@t</option>
                    }
                </select>
            </div>

            <div class="form-group">
                <label>Category Analyst</label>
                <div></div>
                <select>
                    @foreach (var t in _categoryNames)
                    {
                        <option>@t</option>
                    }
                </select>
            </div>

            <div class="flex-fill"></div>

            <a href="/pricing/results/roleWasSelected" class="btn btn-secondary" @onclick=SelectCategory>Select</a>
        </div>

    </div>
}
else
{
    <button class="btn btn-secondary">Unsubscribe</button>

    <!-- Outer Table, Summary Rows -->
    <table class="table table-hover" id="ttt" style="">
        <thead>
        <tr>
            <th scope="col"></th>
            <th scope="col">Item #</th>
            <th scope="col">MFR #</th>
            <th scope="col">Description</th>
            <th scope="col">Lead Time</th>
            <th scope="col">Price</th>
            <th scope="col">FS</th>
            <th scope="col">Ship Cost</th>
            <th scope="col">Link</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var x in _results)
        {
            @if (!_isFiltered || x.Results.MinBy(y => y.CartPrice)!.CartPrice < x.CartPrice)
            {
                <tr>
                    <td class="pointer" @onclick=@(() => x.IsExpanded = !x.IsExpanded)>
                        @if (x.IsExpanded)
                        {
                            <i class="fa-duotone fa-square-minus"></i>
                        }
                        else
                        {
                            <i class="fa-duotone fa-square-plus"></i>
                        }
                    </td>
                    <td>@x.ItemNumber</td>
                    <td>@x.MfrNum</td>
                    <td>@x.Description</td>
                    <td>@x.LeadTime</td>
                    <td class="flex-row">
                        <!-- Comp lower price -->
                        @if (x.Results.MinBy(y => y.CartPrice)!.CartPrice < x.CartPrice)
                        {
                            <span style="color: red;" class="flex-fill">$@x.CartPrice</span>
                        }
                        else
                        {
                            <span class="flex-fill">$@x.CartPrice</span>
                        }
                    </td>
                    <td>
                        @if (x.FreeShipping)
                        {
                            <i class="fa-sharp fa-light fa-check" style="color: grey"></i>
                        }
                    </td>
                    <td>$@x.ShipCost</td>
                    <td class="flex-row">
                        <!-- Wss -->
                        <a href="@x.Url"><i class="fa-duotone fa-arrow-up-right-from-square"></i></a>
                        <span class="flex-fill"></span>
                        <!-- Comp lower price -->
                        @if (x.Results.MinBy(y => y.CartPrice)!.CartPrice < x.CartPrice)
                        {
                            <a style="color: red" href="@x.Results.MinBy(y => y.CartPrice)!.Url"><i class="fa-duotone fa-arrow-up-right-from-square"></i></a>
                        }
                    </td>
                </tr>

                <!-- Results Rows -->
                @if (x.IsExpanded)
                {
                    @foreach (var z in x.Results.OrderBy(x => x.CartPrice))
                    {
                        <tr >
                            <td></td>
                            <td></td>
                            <td></td>
                            <td style="text-align: right">@GetDomainFromUrl(z.Url)</td>
                            <td>@z.LeadTime</td>
                            <td class="flex-row">
                                <!-- Comp lower price -->
                                @if (z.CartPrice < x.CartPrice)
                                {
                                    <span style="color: red;" class="flex-fill">$@z.CartPrice
                                    </span>
                                }
                                else
                                {
                                    <span class="flex-fill">$@z.CartPrice
                                    </span>
                                }
                            </td>
                            <td></td>
                            <td>$@z.ShippingCost</td>
                            <td class="flex-row">

                                <!-- Comp lower price -->
                                @if (z.CartPrice < x.CartPrice)
                                {
                                    <a style="color: red" href="@z.Url"><i class="fa-duotone fa-arrow-up-right-from-square"></i></a>
                                }
                                else
                                {
                                    <a style="color: grey" href="@z.Url"><i class="fa-duotone fa-arrow-up-right-from-square"></i></a>

                                }
                            </td>
                        </tr>


                    }
                }
            }
        }
        </tbody>
    </table>
    <button class="btn @(_isFiltered ? "btn-danger" : "btn-secondary")" @onclick=@(() => _isFiltered = !_isFiltered)>Filter</button>
}

