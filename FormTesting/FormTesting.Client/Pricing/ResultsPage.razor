        {
            ItemNumber = result.ItemNumber;
            Timestamp = result.Timestamp;
            Results = result.Results;
        }

        // full constructor
        //   Item #	MFR #	        DESC	            Lead Time	        CURRENT COST	      LIST	        Cart/Sell Price     	FS	        Ship Cos	Link

        public ItemScrapeResultVm(string itemNumber, string mfrNum, string description, string leadTime, decimal currentCost, decimal list, decimal cartPrice, bool freeShipping, decimal shipCost, string url )
